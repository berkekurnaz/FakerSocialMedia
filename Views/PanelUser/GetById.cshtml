@model User
@{
    ViewData["Title"] = "GetById";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}

<div class="card">
    <div class="card-heading border bottom">
        <br />
        <h4 class="card-title text-center">User Detail</h4>
    </div>
    <div class="card-block">
        <div class="row">
            <div class="col-md-9 ml-auto mr-auto">
                <form asp-controller="" asp-action="" method="post" enctype="multipart/form-data" class="form-horizontal mrg-top-40 pdd-right-30 ng-pristine ng-valid">
                    <div class="form-group">
                        <label>Username</label>
                        <input type="text" class="form-control" asp-for="Username" value="@Model.Username" readonly>
                    </div>
                    <div class="form-group">
                        <label>Password</label>
                        <input type="text" class="form-control" asp-for="Password" value="@Model.Password" readonly>
                    </div>
                    <div class="form-group">
                        <label>Is This User In Admin ?</label>
                        <input type="text" class="form-control" asp-for="IsAdmin" value="@Model.IsAdmin" readonly>
                    </div>
                    <div class="form-group">
                        <label>User Api Key</label>
                        <input type="text" class="form-control" asp-for="ApiKey" value="@Model.ApiKey" readonly>
                    </div>
                </form>
                <center><a href="/PanelUser/GetAll/" class="btn btn-primary"><i class="fa fa-backward" aria-hidden="true"></i> Back Page</a></center><br />
            </div>
        </div>
    </div>
</div>

